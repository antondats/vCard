<!DOCTYPE>
<html class="no-js" lang="en">
<head>
    <title>@ViewBag.Title</title>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width">
    <link rel="shortcut icon" type="image/x-icon" href="~/Content/Images/Others/favicon.ico">
    <link rel="shortcut icon" type="image/png" href="~/Content/Images/Others/favicon.ico">
    <link href='http://fonts.googleapis.com/css?family=Open+Sans:400italic,400,700' rel='stylesheet' type='text/css'>
    <link rel="stylesheet" href="~/Content/CSS/style.css">
</head>
<body>

    <div class="container">
        <header id="navtop">
            <a href="/Profile" class="logo fleft"> <img src="~/Content/Images/Others/logo1.png" alt=""> </a>

            @Html.Partial("Menu")
        </header>
        <div class="home-page main">
            <section class="grid-wrap">
                <header class="grid col-full">
                    <hr>
                    <p class="fleft">@ViewBag.Title</p>
                </header>
                @RenderBody()
            </section>
        </div>
        <!--main-->
        <div class="divide-top">
            <footer class="grid-wrap">
                <ul class="grid col-one-third social">
                    <li><a href="#">RSS</a></li>
                    <li><a href="#">Facebook</a></li>
                    <li><a href="#">Twitter</a></li>
                    <li><a href="#">Google+</a></li>
                    <li><a href="#">Flickr</a></li>
                </ul>
                <div class="up grid col-one-third "> <a href="#navtop" title="Go back up">&uarr;</a> </div>
                <nav class="grid col-one-third ">
                    <ul>
                        <li><a href="#">Home</a></li>
                        <li><a href="#">About</a></li>
                        <li><a href="#">Works</a></li>
                        <li><a href="#">Services</a></li>
                        <li><a href="#">Blog</a></li>
                        <li><a href="#">Contact</a></li>
                    </ul>
                </nav>
            </footer>
        </div>
    </div>
    <!-- Javascript - jQuery -->
    <script src="~/Content/JS/jquery-1.7.2.min.js"></script>
    <!--[if (gte IE 6)&(lte IE 8)]><script src="js/selectivizr.js"></script><![endif]-->
    <script src="~/Content/JS/jquery.flexslider-min.js"></script>
    <script src="~/Content/JS/scripts.js"></script>

    

</body>
</html>